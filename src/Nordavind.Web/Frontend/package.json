{
  "name": "nordavind.web",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run build:dev",
    "build:dev": "webpack -d --watch",
    "build:prod": "webpack -p",
    "start-storybook": "start-storybook -c ./Core/Storybook -p 9876",
    "build-storybook": "build-storybook -c ./Core/Storybook -o ../wwwroot/public/storybook/",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage"
  },
  "browserslist": [
    "IE 11",
    "last 4 versions"
  ],
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/enzyme.config.js",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|gif|png|mp4|mkv|avi|webm|swf|wav|mid)$": "jest-static-stubs/$1"
    }
  },
  "repository": {
    "type": "git",
    "url": "github.com/eirhor/nordavind"
  },
  "author": "Eirik Horvath",
  "license": "MIT",
  "dependencies": {
    "@types/enzyme": "^3.1.8",
    "@types/jest": "^22.1.0",
    "@types/react": "^16.0.35",
    "@types/storybook__react": "^3.0.6",
    "autoprefixer": "^8.1.0",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "expose-loader": "^0.7.4",
    "jest": "^22.4.2",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.1",
    "postcss-pxtorem": "^4.0.1",
    "postcss-sassy-import": "^1.2.3",
    "postcss-scss": "^1.0.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.2",
    "ts-jest": "^22.4.1",
    "ts-loader": "^3.5.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.6.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2",
    "webpack": "^3.11.0"
  },
  "devDependencies": {
    "@storybook/addon-info": "^3.3.14",
    "@storybook/addon-knobs": "^3.3.14",
    "@storybook/addon-viewport": "^3.3.14",
    "@storybook/react": "^3.3.14",
    "storybook-host": "^4.1.5"
  }
}
